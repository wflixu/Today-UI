{
  "name": "use-floating",
  "version": "0.0.13",
  "description": "using floating-ui with vue3 composition api",
  "main": "dist/use-floating.umd.js",
  "module": "dist/use-floating.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/use-floating.es.js",
      "require": "./dist/use-floating.umd.js"
    },
    "./package.json": "./package.json",
    "./src/index.ts": "./src/index.ts"
  },
  "sideEffects": false,
  "files": [
    "dist/",
    "src/",
    "index.d.ts"
  ],
  "scripts": {
    "test": "pnpm vite build",
    "dev": "pnpm vite  --force",
    "buil": "tsc -p ./tsconfig.type.json && pnpm vite build",
    "build": "NODE_ENV=build rollup -c",
    "build:type": "tsc -p ./tsconfig.type.json"
  },
  "keywords": [
    "floating-ui",
    "@floating-ui/dom",
    "vue3",
    "composition api",
    "tooltip",
    "menu",
    "dropdown",
    "contextmenu",
    "modal"
  ],
  "author": "wflixu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wflixu/Today-UI"
  },
  "peerDependencies": {
    "@floating-ui/dom": "^0.4.2",
    "vue": "^3.2.0"
  },
  "dependencies": {
    "@floating-ui/dom": "^0.4.2",
    "vue": "3.2.32"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.3",
    "vite": "^2.9.12",
    "@rollup/plugin-alias": "^3.1.4",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.23",
    "babel-preset-vue": "^2.0.2",
    "jest": "^27.3.1",
    "rollup": "^2.60.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.0",
    "ts-jest": "^27.0.7",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2"
  }
}