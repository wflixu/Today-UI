{
  "name": "today-ui",
  "version": "0.0.3",
  "description": "dropdown menu toolptip powered by vue3, floating-ui",
  "main": "lib/index-lib.js",
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "exports": {
    ".": {
      "import": "./es/index.js",
      "require": "./dist/today-ui.js"
    },
    "./package.json": "./package.json",
    "./src/index.ts": "./src/index.ts"
  },
  "sideEffects": false,
  "files": [
    "dist/",
    "index.d.ts",
    "components/**/*",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "dev": "vite",
    "test": "vite build",
    "build": "cross-env NODE_ENV=production rollup -c ./rollup.config.js && pnpm build:type",
    "build:type": "tsc --emitDeclarationOnly -d --outDir es/  && tsc --emitDeclarationOnly -d --outDir lib/",
    "build:tsc": "pnpm run build:tsc-es && pnpm run build:tsc-esm && pnpm run build:tsc-lib",
    "build:tsc-es": "tsc --emitDeclarationOnly -d --outDir es/",
    "build:tsc-esm": "tsc --emitDeclarationOnly -d --outDir esm/",
    "build:tsc-lib": " tsc --emitDeclarationOnly -d --outDir lib/"
  },
  "keywords": [
    "floating-ui",
    "@floating-ui/dom",
    "vue3",
    "composition api",
    "tooltip",
    "menu",
    "dropdown",
    "contextmenu",
    "modal"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wflixu/Today-UI"
  },
  "author": "wflixu",
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.17.21",
    "use-floating": "^0.0.8",
    "vue": "^3.2.37",
    "vue-router": "^4.0.15"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.182",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.7",
    "@vue/compiler-sfc": "^3.2.33",
    "postcss": "^8.4.12",
    "typescript": "^4.6.3",
    "vite": "^2.9.12"
  }
}