{
  "name": "today-ui",
  "version": "0.0.4",
  "description": "dropdown menu toolptip powered by vue3, floating-ui",
  "main": "dist/today-ui.umd.js",
  "module": "dist/today-ui.es.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/today-ui.es.js",
      "require": "./dist/today-ui.umd.js"
    },
    "./package.json": "./package.json",
    "./src/index.ts": "./src/index.ts"
  },
  "sideEffects": false,
  "files": [
    "dist/",
    "src/",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && pnpm build:type",
    "build:type": "tsc --emitDeclarationOnly -d  -p tsconfig.type.json"
  },
  "keywords": [
    "floating-ui",
    "@floating-ui/dom",
    "vue3",
    "composition api",
    "tooltip",
    "menu",
    "dropdown",
    "contextmenu",
    "modal"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wflixu/Today-UI"
  },
  "author": "wflixu",
  "license": "MIT",
  "dependencies": {
    "@floating-ui/vue": "^0.2.0",
    "lodash": "^4.17.21",
    "vue": "^3.2.0"
  },
  "peerDependencies": {
    "@floating-ui/vue": "^0.2.0",
    "lodash": "^4.17.21",
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.11.18",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.7",
    "@vue/compiler-sfc": "^3.2.33",
    "cross-env": "^7.0.3",
    "i": "^0.3.7",
    "less": "^4.1.3",
    "npm": "^9.3.0",
    "postcss": "^8.4.12",
    "typescript": "^4.6.3",
    "vite": "^2.9.12"
  }
}