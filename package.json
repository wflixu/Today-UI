{
  "name": "today-ui",
  "version": "0.1.0",
  "description": "dropdown menu toolptip powered by vue3, floating-ui",
  "main": "dist/today-ui.umd.js",
  "module": "dist/today-ui.es.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/today-ui.es.js",
      "require": "./dist/today-ui.umd.js"
    },
    "./package.json": "./package.json",
    "./dist/style.css": "./dist/style.css"
  },
  "sideEffects": [
    "*.vue",
    "*.md",
    "dist/*",
    "*.css"
  ],
  "files": [
    "dist/",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build": "vite build && pnpm build:type",
    "bd": "tsc -p tsconfig.app.json",
    "build:type": "vue-tsc  -p tsconfig.type.json",
    "preview": "vite preview --port 5050",
    "test:unit": "vitest --environment jsdom",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "sb:dev": "storybook dev -p 6006",
    "sb:build": "storybook build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.0.2",
    "@storybook/addon-interactions": "^8.0.2",
    "@storybook/addon-links": "^8.0.2",
    "@storybook/blocks": "^8.0.2",
    "@storybook/cli": "^8.0.2",
    "@storybook/vue3": "^8.0.2",
    "@storybook/vue3-vite": "^8.0.2",
    "@tsconfig/node20": "~20.1.2",
    "@types/jsdom": "^21.1.6",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.11.30",
    "@types/postcss-import": "^14.0.3",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/compiler-core": "^3.4.21",
    "@vue/compiler-sfc": "^3.4.21",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.4.0",
    "@vuedx/typescript-plugin-vue": "^0.7.6",
    "cross-env": "^7.0.3",
    "jsdom": "^22.1.0",
    "less": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "postcss-import": "^15.1.0",
    "postcss-nested": "^6.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remark-gfm": "^3.0.1",
    "storybook": "^8.0.2",
    "typescript": "~5.1.6",
    "vite": "^5.2.2",
    "vitest": "^0.33.0",
    "@storybook/test": "^8.0.2",
    "vue-tsc": "^2.0.7"
  },
  "dependencies": {
    "@floating-ui/vue": "^1.0.6",

    "lodash": "^4.17.21",
    "vue": "^3.4.21"
  },
  "peerDependencies": {
    "vue": "^3.3.4"
  },
  "keywords": [
    "vue3",
    "vue",
    "Vue.js",
    "fluent design",
    "fluent",
    "floating-ui",
    "@floating-ui/dom",
    "composition api",
    "tooltip",
    "menu",
    "dropdown",
    "contextmenu",
    "modal",
    "file-tree"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wflixu/Today-UI"
  },
  "author": "wflixu",
  "license": "MIT"
}
