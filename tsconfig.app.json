{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "globals.d.ts",
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "src/**/*.tsx",
    "src/**/*.ts",
    "package.json"
, "src/file-tree/FileTree.story.vue"  ],
  "exclude": [
    "src/**/__tests__/*",
    "src/**/*stories.ts"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "dist",
    "verbatimModuleSyntax": false,
    "strictNullChecks": false,
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  }
}